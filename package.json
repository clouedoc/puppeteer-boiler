{
  "name": "puppeteer-extra-boilerplate",
  "scripts": {
    "dev": "yarn ts-node --transpile-only ./src/app/cmd/dev.ts",
    "test": "yarn jest --watch",
    "db:init": "yarn prisma init",
    "db:migrate": "yarn prisma migrate dev",
    "db:refresh-client": "yarn prisma generate",
    "update": "npx yarn-check-updates -u --packageFile package.json"
  },
  "version": "1.0.0",
  "description": "A batteries included boilerplate for puppeteer-extra.",
  "author": "clouedoc",
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^14.14.35",
    "jest": "^26.6.3",
    "prisma": "^2.19.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^4.0.4",
    "@prisma/client": "^2.20.1",
    "chalk": "^4.1.0",
    "delay": "^5.0.0",
    "dotenv": "^8.2.0",
    "p-event": "^4.2.0",
    "puppeteer": "^5",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-recaptcha": "^3.3.7",
    "puppeteer-extra-plugin-stealth": "^2.7.6",
    "puppeteer-extra-plugin-timezone": "^1.0.3",
    "recaptcha": "^1.2.1",
    "winston": "^3.3.3"
  }
}
