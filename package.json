{
  "name": "puppeteer-extra-boilerplate",
  "scripts": {
    "app": "yarn ts-node --transpile-only ./src/app/app.ts",
    "test": "yarn jest --watch",
    "db:init": "yarn db:migrate:refresh",
    "db:wipe": "rm -f \"./prisma/database.db\" && sqlite3 \"./prisma/database.db\" \"VACUUM;\" && yarn db:clean",
    "db:clean": "rm -rf \"./prisma/migrations\" && rm -rf \"./node_modules/.prisma\"  && rm -rf \"./node_modules/.cache\"",
    "db:migrate": "yarn prisma format && yarn prisma migrate save --name foundation --experimental && npx prisma migrate up --experimental && npx prisma generate",
    "db:migrate:refresh": "yarn db:wipe && yarn db:migrate",
    "db:generate": "npx prisma generate",
    "update": "npx yarn-check-updates -u --packageFile package.json",
  },
  "version": "1.0.0",
  "description": "A batteries included boilerplate for puppeteer-extra.",
  "author": "clouedoc",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.35",
    "jest": "^26.6.3",
    "prisma": "^2.19.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^4.0.4",
    "delay": "^5.0.0",
    "dotenv": "^8.2.0",
    "puppeteer": "^5",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-recaptcha": "^3.3.7",
    "puppeteer-extra-plugin-stealth": "^2.7.6",
    "recaptcha": "^1.2.1",
    "winston": "^3.3.3"
  }
}